{
  "title": "Client Service Performance",
  "tags": ["client-service", "performance"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 4,
  "refresh": "30s",
  "panels": [
    {
      "id": 5,
      "title": "RPS - POST /offers",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(count_over_time({job=\"docker-logs\"} |~ \"request_complete\" |~ \"/offers\" [1m])) / 60",
          "refId": "A",
          "legendFormat": "POST /offers RPS"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 10,
            "pointSize": 5
          }
        }
      }
    },
